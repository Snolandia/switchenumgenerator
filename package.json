{
	"name": "switchenumgenerator",
	"displayName": "Cpp(C++) Switch Case Enum Generator",
	"description": "Generate Switch Case for a variable with type enum",
  "repository": "https://github.com/Snolandia/",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.101.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:cpp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "switchenumgenerator.addCases",
				"title": "Add Cases"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/lodash.uniqwith": "^4.5.6",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/vscode": "^1.102.0",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^9.16.0",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	}
}
